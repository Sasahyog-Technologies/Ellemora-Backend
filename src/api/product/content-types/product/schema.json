{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true
    },
    "subTitle": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "subTitle"
    },
    "originCountry": {
      "type": "string"
    },
    "material": {
      "type": "string"
    },
    "discountable": {
      "type": "boolean"
    },
    "variations": {
      "displayName": "options",
      "type": "component",
      "repeatable": true,
      "component": "variations.options"
    },
    "variants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant.product-variant",
      "mappedBy": "product"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "collections": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::collection.collection",
      "inversedBy": "products"
    },
    "groupBy": {
      "type": "string"
    },
    "template": {
      "type": "string"
    },
    "tags": {
      "type": "text"
    },
    "fit": {
      "type": "string"
    },
    "length": {
      "type": "string",
      "default": "0"
    },
    "sleeveTypes": {
      "type": "string"
    },
    "neckLine": {
      "type": "string"
    },
    "components": {
      "type": "string"
    },
    "numberOfComponents": {
      "type": "string"
    },
    "febric": {
      "type": "string"
    },
    "typeOfWork": {
      "type": "string"
    },
    "care": {
      "type": "string"
    },
    "disclaimer": {
      "type": "text"
    },
    "shippingDisclaimer": {
      "type": "text"
    },
    "media": {
      "type": "component",
      "repeatable": true,
      "component": "variations.color-images"
    },
    "package_weight": {
      "type": "string",
      "default": "0"
    },
    "package_height": {
      "type": "string",
      "default": "0"
    },
    "package_width": {
      "type": "string",
      "default": "0"
    },
    "package_breadth": {
      "type": "string",
      "default": "0"
    },
    "package_length": {
      "type": "string"
    },
    "label": {
      "type": "text"
    }
  }
}
